<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.crm.ssh2.right.entity.RightQx" table="right_qx">
		<id name="rqId" type="java.lang.Integer">
			<column name="rq_id"></column>
			<generator class="native"></generator>
		</id>
	
		<property name="rqrRoleId" type="java.lang.Integer">
			<column name="rqr_role_id"></column>
		</property>
		<property name="rqrCode" type="java.lang.String">
			<column name="rqr_code"></column>
		</property>
		
		
	</class>
</hibernate-mapping>